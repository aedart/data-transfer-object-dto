{
  "name": "aedart/dto",
  "description": "A variation / interpretation of the Data Transfer Object (DTO) design pattern (Distribution Pattern)",
  "keywords": [
    "Data Transfer Object",
    "DTO",
    "Design Pattern"
  ],
  "homepage": "https://bitbucket.org/aedart/data-transfer-object-dto",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "aedart/overload": "~1.3",
    "aedart/util": "~1.2",
    "illuminate/container": "~5.1",
    "illuminate/support": "~5.1"
  },
  "require-dev": {
    "aedart/license": "1.*",
    "aedart/license-file-manager": "1.*",
    "codeception/codeception": "~2.1",
    "symfony/var-dumper": "~2.7",
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "Aedart\\DTO\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
